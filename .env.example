HEADLESS=true
PAGE_LOAD_STRATEGY=eager
DISABLE_IMAGES=true

FIND_TIMEOUT=10
AFTER_SUBMIT_PAUSE=0.8
PAUSE_MIN=0.3
PAUSE_MAX=0.7
BATCH_SIZE=120

INPUT_XLSX=data/comments.xlsx
OUTPUT_XLSX=data/comments_out.xlsx

# USER_AGENT=Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/129.0.0.0 Safari/537.36

# Proxy (optional)
# PROXY_SCHEME=http
# PROXY_XLSX=data/proxies.xlsx
# PROXY_FILE=data/proxies.txt
# PROXY_LIST=http://proxy1:port,http://proxy2:port
# PROXY_URL=http://user:pass@host:port
# Port-only providers:
# PROXY_HOST=proxy.provider.com
# PROXY_USER=username
# PROXY_PASS=password

# Gemini (optional)
# GEMINI_API_KEY=your_key_here
